<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper  namespace="com.bd10.knowledgesystems.dao.others.ServiceHttpDao">
    <select id="queryServiceContent"  resultType="java.lang.String">
        SELECT service_agreement_content FROM `service_agreement`  WHERE  service_agreement_id=1002;
    </select>
    <update id="updataSeerviceHttp">
        UPDATE   service_agreement  SET  service_agreement_content =#{service_agreement_content}  WHERE  service_agreement_id=1002
    </update>

</mapper>